from pylab import *

class Esfera:
    def __init__(self, r, cor='preta'):  # Método Dunder Construtor 
        self.r = r  # Atributo declarado
        self.cor = cor  # Atributo declarado
        
    def area(self):  # Método
        return 4 * pi * self.r**2
    
    def volume(self):  # Método
        return 4/3 * pi * self.r**3





